    e = e || window.event;
    var keyCode = e.which || e.keyCode;
    if (keyCode === 27) {
      self.close();
    }
  });
  zestBind(win, "resize", function () {
    self.setPosition();
  });

  zestBind(win, "message", function (e) {
    if (e.origin !== baseUrl) {
      return;
