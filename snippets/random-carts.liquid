{% assign min = 50 %}
{% assign max = 300 %}
{% assign diff = max | minus: min %}
{% assign randomNumber = "now" | date: "%N" | modulo: diff | plus: min %}
<p class="mb-0 mt-2" id="currently-in-cart" style="color: #03F2AC;">Currently in <span>{{randomNumber}} carts</span></p>
<script>
$('.color-swatch').each(function(ind,el){
  $(el).on('click',function(e){
      if ( $( this ).hasClass( "is-disabled" ) ) {
     document.getElementById("currently-in-cart").style.display = "none";
    }else{
     document.getElementById("currently-in-cart").style.display = "block";
    }
  });
           });
</script>