{% assign min = 50 %}
{% assign max = 300 %}
{% assign diff = max | minus: min %}
{% assign randomNumber = "now" | date: "%N" | modulo: diff | plus: min %}
<p class="" id="currently-in-cart" style="font-weight: 500;font-size: 14px;line-height: 17px;color: #FF030D;">Currently in <span>{{randomNumber}} carts</span></p>