  const target = "custom-lazyloaded-content";
const ids = [];

const observer = new MutationObserver((mutations) => {
  mutations.forEach((mutation) => {
    if (ids.length > 0) {
      // disconnect
      observer.disconnect();
      return;
    }
    if (mutation.type === "childList") {
      const quinnCards = document.querySelectorAll(`.${target} .quinn_cards`);
      if (quinnCards.length > 0) {
        quinnCards.forEach((quinnCard) => {
          ids.push(quinnCard.id);
        });
        renderCards();
      }
    }
  });
});

const config = {
  attributes: true,
  childList: true,
  characterData: true,
  subtree: true,
};

const quinnTargetNode = document.querySelector(`.${target}`);
observer.observe(quinnTargetNode, config);

function renderCards() {
   Quinn.functions.renderCards("LHYrwamwMGtZFFxombAuV", "quinn-cards-layer-3");
}

