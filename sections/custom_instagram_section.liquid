<style>
  span.explore_now_collection_title{
    display:block;
    font-size: 28px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 0.96;
    letter-spacing: -1.12px;
    text-align: center;
    color: #1a2024;
    margin-bottom: 0.5rem;
  }
  .collection_info{
    background: #eff4f7;
    border-bottom-left-radius: 12px;
    border-bottom-right-radius: 12px;
    padding: 1.5rem 0 1rem;
  }

  .custom_instagram_section .MultiCarousel .MultiCarousel-inner .item>div:not(.product__media-image-wrapper){padding: 0px;}
  .custom_instagram_section .section__header{margin: 40px 0px 28px 0px;}
  .custom_instagram_section h3{font-size: 32px;}
  .custom_instagram_section h3 span:after{content: none;}
  </style>
  <!-- Shop by section starts here -->
  <div class="explore_now_section custom_instagram_section container">
    <div class="container MultiCarousel_container">
      <div class="row">
        <div class="col-md-12 section__header text-container d-flex justify-content-between">
          <h3 class="ui2-heading">
