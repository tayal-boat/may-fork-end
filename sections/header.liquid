<script>
 function cart_bubble(){if($('.header__cart-count.header__cart-count--floating.bubble-count').text() != '0'){$('.header__cart-count.header__cart-count--floating.bubble-count').removeClass("d-none")}else{$('.header__cart-count.header__cart-count--floating.bubble-count').addClass("d-none")}}
 cart_bubble();
 document.addEventListener('cart:updated', function(){cart_bubble() });
 {%- if section.settings.gif_logo_time != 0 -%}
   let logo_timer = {{ section.settings.gif_logo_time | times: 1000 }};
     setTimeout(function(){ 
       document.querySelector('.gif_logo.timeout').classList.add("d-none");document.querySelector('.header__logo-image.timein').classList.remove("d-none");
     }, logo_timer);
 {%- endif -%}
   (() => {
     const headerElement = document.getElementById('shopify-section-{{ section.id }}'),
       headerHeight = headerElement.clientHeight,
       headerHeightWithoutBottomNav = headerElement.querySelector('.header__wrapper').clientHeight;

     document.documentElement.style.setProperty('--header-height', headerHeight + 'px');
     document.documentElement.style.setProperty(
       '--header-height-without-bottom-nav',
       headerHeightWithoutBottomNav + 'px'
     );
   })();
   // mobile menu drawer height and top setup

 const documentStyle = getComputedStyle(document.documentElement);
 let mobilemenuHeight =  $('.shopify-section--header .container').height() + $('#shopify-section-announcement-bar').height();
 let mobilemenuHeightforbtt =  $('.shopify-section--header .container').height() + 10;
 document.documentElement.style.setProperty('--mobilemenuHeight', `${mobilemenuHeight}px`);
 document.documentElement.style.setProperty('--mobilemenuHeightforbtt', `${mobilemenuHeightforbtt}px`);
   const targNode = document.getElementById('shopify-section-announcement-bar');
   const config_ = { attributes: true, childList: true, subtree: true };
   const callback_ = (mutationList, observer) => {
   for (const mutation of mutationList) {
    if ($("#shopify-section-announcement-bar").hasClass("announcement-non-sticky") ){
      mobilemenuHeight =  $('.shopify-section--header .container').height() + $('#shopify-section-announcement-bar').height();
    }else{
      mobilemenuHeight =  $('.shopify-section--header .container').height();
    }
    document.documentElement.style.setProperty('--mobilemenuHeight', `${mobilemenuHeight}px`);
    }
   };
   const observer_ = new MutationObserver(callback_);
   observer_.observe(targNode, config_);
</script>


