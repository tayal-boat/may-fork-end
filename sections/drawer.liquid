              if(event.target.getAttribute('show_swatches') == 'false'){
              $(`.stripSwatches_${var_id}`).css('height','max-content');
               $(`.stripSwatches_${var_id}`).css('border-radius','0');
                $(`.stripSwatches_${var_id}`).css('box-shadow','black 1.4px -6px 22px -6px');
