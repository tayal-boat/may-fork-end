            $.ajax({
              type: 'get',
              url: `https://api.postalpincode.in/pincode/${zip}`,
              success: function (response) {
                if (response[0].Status == 'Error') {
                  alert(response[0].Message);
                  $('#city').val('');
                  $('#state').val('');
                } else {
                  if (response[0].PostOffice[0].Block == 'NA') {
                    $('#checkout_shipping_address_city').val(response[0].PostOffice[0].District);
                  } else {
                    $('#checkout_shipping_address_city').val(response[0].PostOffice[0].Block);
                  }
  
                  $('#checkout_shipping_address_province option').each(function () {
                    let rtext = response[0].PostOffice[0].State.replace(' & ', ' and ');
                    if ($(this).text() == rtext) {
                      $(this).prop('selected', true);
                    }
                  });
                }
              },
              error: function (response) {
                console.log(response);
                alert('something went wrong');
              },
            });
          }
        });
        $('#checkout_shipping_address_zip').on('change', async function () {
          var pin_c = $('#checkout_shipping_address_zip')[0].value;
  
          const data = JSON.stringify({
            query: `	query {pincode(
                pin: "${pin_c}"
              ){
                city
                      serviceable
            state
              }
              }`,
          });
  
          const response = await fetch('https://boatfcapi.farziengineer.co/graphql/', {
            method: 'post',
            body: data,
            headers: {
              'Content-Type': 'application/json',
            },
          });
  
          const json = await response.json();
          if (json.data.pincode != null) {
            if (json.data.pincode.serviceable) {
              $('#checkout_shipping_address_city')[0].value = json.data.pincode.city;
              if ($('.wrong_pincode').length > 0) {
                $('.wrong_pincode').remove();
              }
