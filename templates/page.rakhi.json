    "16594432198401d7ca": {
      "type": "custom-html",
      "settings": {
        "subheading": "",
        "title": "",
        "html": "<style>\n  #campModal {\n    z-index: 9999;\n    background: #0009;\n    padding: 0 !important;\n  }\n  #campModal .modal-dialog {\n    top: 20%;\n  }\n  #campModal .close {\n    position: absolute;\n    top: 0;\n    right: 0;\n    width: 30px;\n    height: 30px;\n    background: #eb1c24;\n    color: #fff;\n    line-height: 30px;\n    font-weight: normal;\n    opacity: 1;\n    text-align: center;\n    font-size: 30px !important;\n  }\n  #campModal .close:focus {\n    border: 0;\n    outline: none;\n  }\n  .error-msg-sale,\n  .thankyou-msg-sale,\n  .aluser-msg-sale {\n    text-align: center;\n    margin: 0;\n    font-size: 12px;\n    color: #fff;\n    opacity: 0.8;\n  }\n  .sale-btn:focus {\n    outline: none;\n  }\n  #campModal img {\n    display: block;\n  }\n  #campModal .modal-content {\n    border: 0;\n  }\n  .modal-form {\n    background: #000;\n  }\n  #campModal input {\n    border: 1px solid #ccc;\n    background: transparent;\n    color: #ccc;\n    margin-right: 4px;\n    padding: 0 12px;\n    height: 45px;\n  }\n  #campModal input::-webkit-input-placeholder {\n    color: #ccc;\n  }\n  #campModal .sale-btn:hover {\n    background: #eb1c24 !important;\n  }\n  img.only-desktop {\n    width: 100vw;\n  }\n  @media screen and (max-width: 600px) {\n    #campModal .modal-dialog {\n      top: 25%;\n    }\n    #campModal {\n      transform: translateY(0%);\n    }\n  }\n<\/style>\n<div id=\"campModal\" class=\"modal fade\" role=\"dialog\">\n  <div class=\"modal-dialog\">\n    <div class=\"modal-content\">\n      <div class=\"modal-body p-0\">\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\">×<\/button>\n        <div class=\"modal-body-content\">\n          <div class=\"modal-image\">\n            <img loading=\"lazy\" class=\"image-overlay_image\" src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0057\/8938\/4802\/files\/pop-up_0a4183cb-fab8-49d0-92ac-652f3fa52a45.jpg?v=1661427888\">\n          <\/div>\n          <div class=\"modal-form p-3\">\n            <div class=\"sale-pop-form\">\n              <div class=\"form-group\" style=\"margin: 0\">\n                <div class=\"myform\" style=\"display: flex; margin: 0; justify-content: center\">\n                  <input style=\"\n                      border-radius: 5px;\n                      max-width: 65%;\n                      padding: 0 8px;\n                      font-size: 12px !important;\n                      width: 100%;\n                    \" type=\"email\" name=\"email\" id=\"sale_email\" placeholder=\"Email Address\">\n                  <button style=\"\n                      border-radius: 5px;\n                      background: transparent;\n                      border: 1px solid #ff0000;\n                      color: #fff;\n                      height: 45px;\n                      line-height: normal;\n                    \" class=\"button sale-btn\" type=\"submit\">\n                    Get Notified\n                  <\/button>\n                <\/div>\n              <\/div>\n            <\/div>\n          <\/div>\n        <\/div>\n      <\/div>\n    <\/div>\n  <\/div>\n<\/div>\n<script>\n    $(document).ready(function () {\n      let camp = \"\/boat-days-2022\";\n      if (\n        window.location.href.indexOf(camp) > -1 &&\n        sessionStorage.getItem(\"popUpPoped\") === null\n      ) {\n        $(\"#campModal\").modal(\"show\");\n        sessionStorage.setItem(\"popUpPoped\", \"poped\");\n      }\n    });\n    $(\".sale-btn\").click(function () {\n      var e = document.getElementById(\"sale_email\").value;\n      \"\" !== e && \/^([\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4})?$\/.test(e)\n        ? localStorage.getItem(\"userEmailIdSale\") === e\n          ? $(\".form-group\").append(\n              '<p class=\"aluser-msg-sale\">You are already subscribed!!<\/p>'\n            )\n          : (localStorage.setItem(\"userEmailIdSale\", e),\n            clevertap.onUserLogin.push({\n              Site: { Email: e, \"MSG-email\": !0 },\n            }),\n            clevertap.event.push(`Notify Me ${camp}`, {\n              Email: e,\n              Source: `campaign-${camp}`,\n            }),\n            $(\"#campModal .form-group\").append(\n              '<p class=\"thankyou-msg-sale\">Thank You for subscribing!<\/p>'\n            ))\n        : $(\".form-group\").append(\n            '<p class=\"error-msg-sale\">Please enter a valid email Id!<\/p>'\n          );\n      setTimeout(function () {\n        $(\n          \".error-msg-sale, .thankyou-msg-sale, .aluser-msg-sale\"\n        ).remove();\n      }, 5e3);\n    });\n  <\/script>",
        "add_vertical_spacing": false,
        "background_type": "full_width",
        "background": "rgba(0,0,0,0)",
        "text_color": "rgba(0,0,0,0)"
      }
    }
