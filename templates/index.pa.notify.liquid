            triggerEvent:function(el, event) {
                if ("createEvent" in document) {
                    var evt = document.createEvent("HTMLEvents");
                    evt.initEvent(event, false, true);
                    el.dispatchEvent(evt);
                }
                else
                    el.fireEvent(event);
            }
        };
