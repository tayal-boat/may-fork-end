xt, 3, { opacity: 1 }, { opacity: 0 });\n\n      let scene2 = new ScrollMagic.Scene({\n        duration: 3000,\n        triggerElement: intro,\n        triggerHook: 0\n      })\n        .setTween(textAnim)\n        .addTo(controller);\n\n      \/\/Video Animation\n      let accelamount = 0.3;\n      let scrollpos = 0;\n      let delay = 0;\n\n      scene.on(\"update\", e => {\n        scrollpos = e.scrollPos \/ 1000;\n      });\n\n      setInterval(() => {\n        delay += (scrollpos * 4 - delay) * accelamount;\n        \/\/console.log(scrollpos, delay);\n\n        video.currentTime = delay;\n      }, 40);\n      console.log(video);\n    <\/script>",
