        "liquid": "<div class=\"container\">\n\t<div class=\"row\">\n\t\t<div class=\"col-md-7 col-sm-12\">\n\t\t\t<div class=\"image_section\">\n\t\t\t\t<img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0057\/8938\/4802\/files\/Bulk-Order-Page-Desktop_01.jpg?v=1693222306\" class=\"only-desktop\">\n\t\t\t\t<img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0057\/8938\/4802\/files\/Bulk-Order-Page-Mobile_01.jpg?v=1693222306\" class=\"mobile-only\">\n\t\t\t<\/div>\n\t\t<\/div>\n\t\t<div class=\"col-md-5\">\n\t\t\t<div class=\"container mt-5\">\n\t\t\t    <div id=\"bulk_order_form\" >\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"fullName\">Full Name<\/label>\n\t\t\t            <input type=\"text\" class=\"form-control\" id=\"fullName\" name=\"fullName\" pattern=\"[A-Za-z]+( [A-Za-z]+)*\" required>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please enter your full name.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"email\">Email<\/label>\n\t\t\t            <input type=\"email\" class=\"form-control\" id=\"email\" name=\"email\" required>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please enter a valid email address.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"phone\">Phone<\/label>\n\t\t\t            <input type=\"tel\" class=\"form-control\" id=\"phone\" name=\"phone\" pattern=\"[0-9]{10}\" required>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please enter a valid 10-digit phone number.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"pincode\">Pincode<\/label>\n\t\t\t            <input type=\"text\" class=\"form-control\" id=\"pincode\" name=\"pincode\" required>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please enter a valid pincode.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"city\">City<\/label>\n\t\t\t            <input type=\"text\" class=\"form-control\" id=\"city\" name=\"city\" readonly>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"state\">State<\/label>\n\t\t\t            <input type=\"text\" class=\"form-control\" id=\"state\" name=\"state\" readonly>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"company\">Company<\/label>\n\t\t\t            <input type=\"text\" class=\"form-control\" id=\"company\" name=\"company\" required>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please enter a company name.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"quantity\">Quantity<\/label>\n\t\t\t            <input type=\"number\" class=\"form-control\" id=\"quantity\" name=\"quantity\" min=\"1\" required>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please enter a valid quantity.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <div class=\"form-group\">\n\t\t\t            <label for=\"requirement\">Requirement<\/label>\n\t\t\t            <textarea class=\"form-control\" id=\"requirement\" name=\"requirement\" rows=\"4\" required><\/textarea>\n\t\t\t            <div class=\"invalid-feedback\">\n\t\t\t                Please describe your requirement.\n\t\t\t            <\/div>\n\t\t\t        <\/div>\n\t\t\t        <input type=\"submit\" class=\"button button--secondary button--full custom_btn\" value=\"Submit\" \/>\n\t\t\t        <a href=\"\" id=\"mail_link\" style=\"opacity: 0;height: 0px; width: 0px;\" target=\"blank\">.<\/a>\n\t\t\t    <\/div>\n\t\t\t<\/div>\n\n\t\t\t<style>\n\t\t\t\t.err_msg{margin-bottom: 2em;color: #f00;font-size: 12px;display: block;}\n\t\t\t\t.custom_btn{font-size: 18px;height: auto;width: auto;font-weight: 500;background: #1a2024;margin-bottom: 2rem;}\n\t\t\t<\/style>\n\n\t\t\t<script>\n\n\t\t\t\t$(document).ready(function(){\n\n\t\t\t\t\t$('#bulk_order_form input[type=\"submit\"]').on('click', function(e){\n\t\t\t\t\t\te.preventDefault();\n\n\t\t\t\t\t\tlet fullName = $('#fullName').val();\n\t\t            \tlet email = $('#email').val();\n\t\t            \tlet phone = $('#phone').val();\n\t\t            \tlet pincode = $('#pincode').val();\n\t\t            \tlet city = $('#city').val();\n\t\t            \tlet state = $('#state').val();\n\t\t            \tlet company = $('#company').val();\n\t\t            \tlet quantity = $('#quantity').val();\n\t\t            \tlet requirement = $('#requirement').val();\n\n\t\t            \tif( fullName != '' && email != '' && phone != '' && pincode != '' && city != '' && state != '' && company != '' && quantity != '' && requirement != ''  ){\n\n\n\t\t            \t\tlet mail_to_content = \"mailto:Corporate@imaginemarketingindia.com?subject=BoAt Bulk Orders&body=Hi,%0d%0a%0d%0aI would like to request a bulk shipment of boAt products.%0d%0a%0d%0aFull Name: \" + fullName + \"%0d%0a%0d%0aEmail: \" + email + \"%0d%0a%0d%0aPhone:  \" + phone + \"%0d%0a%0d%0aPincode: \" + pincode + \"%0d%0a%0d%0aCity: \" + city + \"%0d%0a%0d%0aState: \" + state + \"%0d%0a%0d%0aCompany: \" + company + \"%0d%0a%0d%0aQuantity: \" + quantity + \"%0d%0a%0d%0aRequirement: \"+requirement;\n\n\t\t            \t\t$('#mail_link').attr('href', mail_to_content);\n\t\t            \t\tlocation.reload(true);\n\t\t            \t\t\n\t\t\t            \twindow.location.replace(mail_to_content);\n\t\t            \t}else{\n\t\t            \t\t$('#bulk_order_form').prepend('<span class=\"err_msg\">All the fields are required<\/span>');\n\t\t            \t\tsetTimeout(function(){\n\t\t\t\t\t\t\t\t$('.err_msg').remove();\n\t\t\t\t\t\t\t\t$('#bulk_order_form .custom_btn').removeAttr('disabled');\n\t\t\t\t\t\t\t}, 5000);\n\t\t            \t}\n\n\t\t            \t$('#bulk_order_form .custom_btn').attr('disabled', 'disabled');\n\n\t\t\t\t\t});\n\n\t\t\t\t\t$('#pincode').keyup(function () {\n\t\t\t\t\t    var zip = $(this).val();\n\t\t\t\t\t    if (zip.length == 6) {\n\t\t\t\t\t        $.ajax({\n\t\t\t\t\t            type: 'get',\n\t\t\t\t\t            url: `https:\/\/api.postalpincode.in\/pincode\/${zip}`,\n\t\t\t\t\t            success: function (response) {\n\t\t\t\t\t                if (response[0].Status == 'Error') {\n\t\t\t\t\t                    alert(response[0].Message);\n\t\t\t\t\t                    $('#city').val('');\n\t\t\t\t\t                    $('#state').val('');\n\t\t\t\t\t                } else {\n\t\t\t\t\t                    if (response[0].PostOffice[0].Block == 'NA') {\n\t\t\t\t\t                        $('#city').val(response[0].PostOffice[0].Block);\n\t\t\t\t\t                        $('#state').val(response[0].PostOffice[0].Circle);\n\t\t\t\t\t                    } else {\n\t\t\t\t\t                    \t$('#city').val(response[0].PostOffice[0].Block);\n\t\t\t\t\t                        $('#state').val(response[0].PostOffice[0].Circle);\n\t\t\t\t\t                    }\n\n\t\t\t\t\t                    \/*$('#checkout_shipping_address_province option').each(function () {\n\t\t\t\t\t                        let rtext = response[0].PostOffice[0].State.replace(' & ', ' and ');\n\t\t\t\t\t                        if ($(this).text() == rtext) {\n\t\t\t\t\t                            $(this).prop('selected', true);\n\t\t\t\t\t                        }\n\t\t\t\t\t                    });*\/\n\t\t\t\t\t                }\n\t\t\t\t\t            },\n\t\t\t\t\t            error: function (response) {\n\t\t\t\t\t                alert('something went wrong');\n\t\t\t\t\t            },\n\t\t\t\t\t        });\n\t\t\t\t\t    }\n\t\t\t\t\t});\n\n\t\t\t\t});\n\n\t\t\t<\/script>\n\t\t<\/div>\n\t<\/div>\n<\/div>",
